# Azure OpenAI configuration
OPENAI_API_KEY=your_openai_api_key
OPENAI_API_BASE=your_openai_api_base
OPENAI_API_TYPE=azure
OPENAI_API_VERSION=your_openai_api_version
OPENAI_DEPLOYMENT_NAME=your_deployment_name

# Flask configuration
SECRET_KEY=your_secret_key_here

# MongoDB configuration
MONGODB_URL=mongodb://admin:password@mongodb:27017/lms?authSource=admin

# Azure resource info (not used directly by OpenAI SDK, but kept for reference)
AZURE_ENV_NAME=your_azure_env_name
AZURE_LOCATION=your_azure_location
AZURE_SUBSCRIPTION_ID=your_subscription_id
AZURE_EXISTING_AIPROJECT_ENDPOINT=your_aiproject_endpoint
AZURE_EXISTING_AIPROJECT_RESOURCE_ID=your_aiproject_resource_id
AZD_ALLOW_NON_EMPTY_FOLDER=true